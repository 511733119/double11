<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
          "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<!-- Generated by MyEclipse Hibernate Tools.                   -->
<hibernate-configuration>

	<session-factory>
		<property name="dialect">
			org.hibernate.dialect.MySQLDialect
		</property>
		<!-- 配置显示sql语句 -->
		<property name="hibernate.show_sql">true</property>
		<!--让输出的sql语句格式化 -->
		<property name="format_sql">true</property>
		

		<mapping resource="com/hjc/double11/model/Category.hbm.xml" />
		<mapping resource="com/hjc/double11/model/Product.hbm.xml" />
		<mapping resource="com/hjc/double11/model/Forder.hbm.xml" />
		<mapping resource="com/hjc/double11/model/Sorder.hbm.xml" />
		<mapping resource="com/hjc/double11/model/User.hbm.xml" />
		<mapping resource="com/hjc/double11/model/Pack.hbm.xml" />

		
	</session-factory>

</hibernate-configuration>